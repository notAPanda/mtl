{{#each category}}
    <div class="row">
        <div class="col-xs-12">
            <h2>{{title}}</h2>
            <div class="row">
                {{#each apps}}
                    <div class="col-xs-12 col-md-4">
                        <div class="card">
                            <div class="card-block">
                                <h4 class="card-title">{{title}}</h4>
                                <a class="card-link"href="{{url}}">{{url}}</a>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
    </div>
{{/each}}